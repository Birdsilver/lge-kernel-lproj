 	{ 1, 1400000, ACPU_PLL_4, 6, 0, 175000, 3, 7, 200000 },
+#ifdef CONFIG_MSM7X27A_CRAZYOVERCLOCK
+	{ 1, 1500000, ACPU_PLL_4, 6, 0, 187500, 3, 7, 200000 },
