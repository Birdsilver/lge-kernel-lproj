index cd9e36a..802a70c 100644
--- a/arch/arm/configs/cyanogenmod_m4_defconfig
+++ b/arch/arm/configs/cyanogenmod_m4_defconfig
@@ -461,8 +461,8 @@ CONFIG_MSM_DALRPC=y
 # CONFIG_MSM_DALRPC_TEST is not set
 CONFIG_MSM_CPU_FREQ_SET_MIN_MAX=y
 CONFIG_MSM_CPU_FREQ_MAX=800000
-CONFIG_MSM_CPU_FREQ_MIN=61440
-# CONFIG_MSM7X27A_OVERCLOCK is not set
+CONFIG_MSM_CPU_FREQ_MIN=50000
+CONFIG_MSM7X27A_OVERCLOCK=y
 # CONFIG_MSM_AVS_HW is not set
 # CONFIG_MSM_HW3D is not set
